{
  "name": "interview-node",
  "version": "1.0.0",
  "description": "Interview test: Design a webapp with Node.JS",
  "main": "src/server/index.js",
  "scripts": {
    "start": "concurrently \"yarn run server\" \"yarn run client\"",
    "server": "NODE_ENV=development nodemon src/server/index.js",
    "client": "NODE_ENV=development webpack-dev-server --mode development --devtool inline-source-map --hot",
    "db:migrate": "knex migrate:latest"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": "git@github.com:calllevels/interview-node.git",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "knex": "^0.14.6",
    "pg": "^7.4.3",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "babel-loader": "^8.0.0-beta",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
